{
	"name": "discord.js-bot-structure",
	"version": "1.0.0",
	"description": "A useful discord.js-bot-structure",
	"main": "main.ts",
	"scripts": {
		"test": "tsc --noEmit",
		"compile": "tsc",
		"watch": "tsc --watch",
		"start": "tsc && npm run dev",
		"entrypoint": "npx prisma migrate deploy && npm run start",
		"dev": "node dist/index.js",
		"lint": "eslint .",
		"prettier": "prettier --check **/*.ts",
		"check": "yarn lint && yarn prettier && yarn test",
		"format": "prettier --write **/*.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/warix8/discord.js-bot-structure.git"
	},
	"author": "Warix",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/warix8/discord.js-bot-structure/issues"
	},
	"homepage": "https://github.com/warix8/discord.js-bot-structure#readme",
	"contributors": [
		"Warix",
		"Ota"
	],
	"node": ">=16.6.0",
	"dependencies": {
		"@prisma/client": "^6.19.0",
		"asciiart-logo": "^0.2.7",
		"discord.js": "^14.25.1",
		"dotenv": "^17.2.3",
		"node-cron": "^4.2.1"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.6.2",
		"prisma": "^6.19.0",
		"typescript": "^5.9.3"
	}
